                    {
                        "name": "auth_token",
                        "in": "header",
                        "type": "string",
                        "description": "An authentication token returned upon login"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success."
                    },
                    "400": {
                        "description": "Invalid argument."
                    },
                    "409": {
                        "description": "Authentication failed."
                    },
                    "default": {
                        "description": "Unexpected error."
                    }
                }
            }
        },
        "/exists": {
            "get": {
                "tags": [
                    "user"
                ],
